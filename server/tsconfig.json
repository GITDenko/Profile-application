{
  "compilerOptions": {
    "baseUrl": "./src",
    "outDir": "./dist",
    "module": "esnext",
    "target": "esnext",
    "lib": ["esnext", "WebWorker"],
    "alwaysStrict": true,
    "strict": true,
    "preserveConstEnums": true,
    "moduleResolution": "node",
    "sourceMap": true,
    "esModuleInterop": true,
    "paths": {
      "Services": [
        "./services"
      ],
      "Util": [
        "./util"
      ],
      "Types": [
        "./types"
      ],
    },
    "types": [
      "@cloudflare/workers-types",
      "@types/jest",
      "@types/service-worker-mock",
      "@types/node",
    ]
  },
  "include": ["src"],
  "exclude": ["node_modules", "dist", "test"]
}
